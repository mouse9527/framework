{
  "properties": [
    {
      "name": "security.jwt.verify-key",
      "description": "jwt verify public key path. eg: classpath:public.pem",
      "type": "java.lang.String"
    },
    {
      "name": "security.jwt.verify-algorithm",
      "description": "jwt verify algorithm. default: SHA1WithRSA",
      "type": "java.lang.String",
      "defaultValue": "SHA1WithRSA"
    },
    {
      "name": "security.jwt.decrypt-key",
      "description": "jwt decrypt public key path. eg: classpath:public.pem",
      "type": "java.lang.String"
    },
    {
      "name": "security.jwt.decrypt-transformation",
      "description": "jwt decrypt transformation default: RSA",
      "type": "java.lang.String",
      "defaultValue": "RSA"
    },
    {
      "name": "security.jwt.enable-authenticate",
      "description": "enable QueryApplication and CommandApplication authenticate",
      "type": "java.lang.Boolean"
    },
    {
      "name": "security.jwt.enable-web-token-filter",
      "description": "enable TokenLoaderFilter AutoConfiguration",
      "type": "java.lang.Boolean"
    }
  ]
}
